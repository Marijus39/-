# Задача C

a = input()
print(f'{a} \n' * 3)

# Задача D

kg = 2.5
summ = 38
a = int(kg * summ)
cash = int(input())
print(cash - a)

# Задача E

cost = int(input())
weight = int(input())
money = int(input())
print(money - weight * cost)

# Задача F

name = input()
c = int(input())
w = int(input())
rub = int(input())
print("Чек")
print(f'{name} - {w}кг - {c}руб/кг')
print(f'Итого: {w * c}руб')
print(f'Внесено: {rub}руб')
print(f'Сдача: {rub - w * c}руб')

# Задача G

a = int(input())
print(f"Купи слона! \n" * a)

# Задача H

N = int(input())
s = input()
print(f'Я больше никогда не буду писать "{s}"! \n' * N)

# Задача I

N = int(input())
M = int(input())
print(int(M * N / 2))

# Задача J

name = input()
number = input()
print(f'Группа №{number[0]}.')
print(f'{number[2]}. {name}.')
print(f'Шкафчик: {number}.')
print(f'Кроватка: {number[1]}.')

# Задача K

a = input()
print(f'{a[1]}{a[0]}{a[3]}{a[2]}')

# Задача L

n = int(input())
m = int(input())
a = n % 10
b = m % 10
c = n // 10 % 10
d = m // 10 % 10
e = n // 100 % 10
f = m // 100 % 10
g = (a + b) % 10
h = (c + d) % 10
i = (e + f) % 10
print(f'{i}{h}{g}')

# Задача M

kids = int(input())
candy = int(input())
print(candy // kids)
print(candy % kids)

# Задача N

red = int(input())
green = int(input())
blue = int(input())
print(red + blue + 1)

# Задача O

N = int(input())
M = int(input())
T = int(input())
newM = str((M + T) % 60)
newN = str((N + (M + T) // 60) % 24)
print(f'{newN:0>2}:{newM:0>2}')

# Задача P

a = int(input())
b = int(input())
c = int(input())
print(f'{(b - a) / c:.2f}')
